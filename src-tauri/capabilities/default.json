{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "local": true,
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-open",
    "opener:default",
{
  "identifier": "opener:allow-open-path",
  "allow": [
    {"path": "**"}
  ]
},
"opener:allow-reveal-item-in-dir",
    "store:default"
  ]
}
